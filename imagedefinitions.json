{
    "family": "sample-app-service",
    "containerDefinitions": [
        {
            "name": "sample-app-service",
            "image": "628424922629.dkr.ecr.ap-northeast-1.amazonaws.com/my-image:latest",
            "cpu": 10,
            "memory": 128,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "entryPoint": [
                "/usr/sbin/apache2",
                "-D",
                "FOREGROUND"
            ],
            "essential": true
        }
    ]
}